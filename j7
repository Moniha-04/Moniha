package basics;

import java.util.Scanner;

public class array3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b[]=new int[a];
		int sum=0;
		for(int i=0;i<a;i++)
		{
			b[i]=sc.nextInt();
		}
		for(int i=0;i<a;i++) {
			sum=sum+b[i];
		}
		double avg=sum/a;
		System.out.println("avg"+avg);

	}

}
