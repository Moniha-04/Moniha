package basics;

import java.util.Scanner;

public class array4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b[]=new int[a];
		int max=b[0];
		for(int i=0;i<a;i++) {
			b[i]=sc.nextInt();
		}
		for(int i=0;i<a;i++)
		{
			if(b[i]>max) {
				max=b[i];
			}
			
		}
		System.out.println("max"+max);
		
	}

}
